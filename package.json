{
  "name": "nw-tools",
  "version": "0.0.2",
  "description": "封装得通用的工具函数",
  "packageManager": "npm@6.14.15",
  "main": "dist/nw-utils.cjs.js",
  "module": "dist/nw-utils.esm.js",
  "scripts": {
    "prepare": "husky install",
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "doc": "typedoc  --options typedoc.json",
    "test": "jest --no-cache --collect-coverage",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/wallowyou/nw-utils",
  "bugs": {
    "url": "https://github.com/wallowyou/nw-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wallowyou/nw-utils.git"
  },
  "keywords": [
    "utils",
    "nw",
    "typescript"
  ],
  "author": "wallowyou <wallowyou@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.2",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "dependencies": {}
}
